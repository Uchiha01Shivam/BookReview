@model BookReviewWeb.Models.FAQ

<h2>Create FAQ</h2>
<form method="post" action="Edit" enctype="multipart/form-data">

    <input type="hidden" id="Id" name="Id" value="@Model.Id" class="form-control" />
    <div class="form-group">
        <label for="question">Question</label>
        <input type="text" id="question" name="question" value="@Model.question" class="form-control">
    </div>

    <div class="form-group">
        <label for="answer">Answer</label>
        <input type="text" id="answer" name="answer" value="@Model.answer" class="form-control">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>